    "apps/**"
  ],
  "resolutions": {
    "wrap-ansi": "7.0.0",
    "string-width": "4.2.3",
    "strip-ansi": "6.0.1"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  }
