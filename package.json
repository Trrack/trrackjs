    "@commitlint/cz-commitlint": "^17.4.2",
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@jscutlery/semver": "^2.25.2",
    "@mui/icons-material": "^5.8.0",
    "@mui/material": "^5.8.1",
    "@testing-library/react": "13.4.0",
