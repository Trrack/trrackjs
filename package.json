    "postcss": "^8.4.20",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "regenerator-runtime": "0.13.7",
    "tailwindcss": "^3.2.4",
    "tslib": "^2.3.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
