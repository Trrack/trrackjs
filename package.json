  "scripts": {
    "dev:all": "nx run-many --target=serve --projects=react-trrack-example,rtk-trrack-example --output-style=stream",
    "test:all:watch": "nx run-many --target=test --projects=core,redux --watch",
    "prepare": "husky install",
